ITEM     "&Restore Status..."                               ,geocsuxa_gxnet.dll(Geosoft.GX.CS.UXA.UxaEnableAllInteractiveImageView;Run)
SEPARATOR
ITEM     "&Show All Control Windows..."                     ,geocsuxa_gxnet.dll(Geosoft.GX.CS.UXA.UxaShowAllInteractiveImageView;Run)
ITEM     "&Hide All Control Windows..."                     ,geocsuxa_gxnet.dll(Geosoft.GX.CS.UXA.UxaHideAllInteractiveImageView;Run)
SEPARATOR
ITEM     "Save Existing Viewer Parameters..."               ,geocsuxa_gxnet.dll(Geosoft.GX.CS.UXA.UxaSaveViews;Run)
ITEM     "&Load Viewer Parameters..."                       ,geocsuxa_gxnet.dll(Geosoft.GX.CS.UXA.UxaLoadViews;Run)


MENU "Static Data/Utilities"
/ 
ITEM     "Save Database Working View..."                    ,savedbvu.gx                                        {?gdb}
